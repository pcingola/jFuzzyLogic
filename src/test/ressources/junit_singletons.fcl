FUNCTION_BLOCK testVariables

VAR_INPUT				// Define input variables
	inVar : REAL;
END_VAR

// Pairwise linear
FUZZIFY inVar
	TERM poor := 2;
	TERM good := 5;
	TERM excellent := 8;
END_FUZZIFY

END_FUNCTION_BLOCK

